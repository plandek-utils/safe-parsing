{
	"name": "@plandek-utils/safe-parsing",
	"version": "1.0.0",
	"description": "Safe parsing utilities for strings and floats",
	"type": "module",
	"main": "dist/mod.js",
	"types": "dist/mod.d.ts",
	"files": ["dist", "src"],
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "tsup",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"test:coverage": "vitest run --coverage",
		"lint": "biome lint src",
		"format": "biome format . --write",
		"clean": "rm -rf dist",
		"prepare": "npm run build",
		"prepublishOnly": "npm run test && npm run lint"
	},
	"keywords": ["parsing", "string", "float", "typescript", "utilities"],
	"author": "plandek-utils",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/plandek-utils/safe-parsing.git"
	},
	"bugs": {
		"url": "https://github.com/plandek-utils/safe-parsing/issues"
	},
	"homepage": "https://github.com/plandek-utils/safe-parsing#readme",
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@commitlint/cli": "^19.6.1",
		"@commitlint/config-conventional": "^19.6.0",
		"@vitest/coverage-v8": "^3.0.2",
		"tsup": "^8.3.5",
		"typescript": "^5.7.3",
		"vitest": "^3.0.2"
	},
	"dependencies": {
		"@plandek-utils/safe-compact": "^2.0.1",
		"@plandek-utils/ts-parse-dayjs": "^6.3.0",
		"dayjs": "^1.11.13"
	}
}
